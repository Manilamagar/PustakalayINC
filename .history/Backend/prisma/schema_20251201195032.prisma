generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "mysql"
   url      = env("DATABASE_URL")
}
model User {
  id         Int      @id @default(autoincrement())
  name       String?
  email      String   @unique
  password   String
  roll       String
  phone      Int
  Address    String
}
model Student{
STDid         Int      @id @primary(autoincrement())
STDName       String?
STDClass      String
STDFaculty    String
STDRoll       String
}

model Borrow{
  STDid       Int      @id @primary(autoincrement())
  Borrowid    Int      
  Bookid      Int
  Borrow_date Datetime
  return-date Datetime
  status      Borrow,returned
  }